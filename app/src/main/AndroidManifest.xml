    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.greenbotsite.uiGimmicks.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name="com.greenbotsite.uiGimmicks.AnimationDemoActivity"/>
        <activity android:name="com.greenbotsite.uiGimmicks.VectorDemoActivity"/>
        <activity android:name="com.greenbotsite.uiGimmicks.TransitionSourceActivity"  android:theme="@style/TransitionApptheme"/>
        <activity android:name="com.greenbotsite.uiGimmicks.TransitionDestinationActivity" android:theme="@style/TransitionApptheme"/>
        <activity android:name="com.greenbotsite.uiGimmicks.CustomViewActivity"/>
        <activity android:name="com.greenbotsite.uiGimmicks.FragmentTransitionActivity"/>


    </application>

</manifest>